<ul id="call_escalations_ul" class="no_bullet_list no_indent"> <% @call_list.call_escalations.each do |call_escalation| %>
  <li id="call_escalation_<%= call_escalation.id %>" class="ui-state-default ui-corner-all">
    <span class="ui-icon ui-icon-arrow-4" style="display:inline-block"></span>
    <%= call_escalation.user.username + " | " + call_escalation.user.email + " | " + call_escalation.user.phone_number  %> | Retry: <%= call_escalation.retry %> |
    <%= link_to 'Remove', call_list_call_escalation_path(call_escalation.call_list.id, call_escalation.id), :remote => true, :method => 'delete', :confirm => "Are you sure?" %>
  </li>
<% end %></ul>
