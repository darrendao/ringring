<?xml version="1.0" encoding="UTF-8"?>
<Response>
    <Pause length="1"/>
    <Gather action="<%= @base_url %>/complete_call.xml" timeout="20">
        <Say>To accept this call, press any key, followed by the pound sign.</Say>
        <Pause length="5"/>
        <Say>Are you still there? To accept the call, press any key, followed by the pound sign.</Say>
    </Gather>
    <Say>We didn't receive any input. Goodbye!</Say>
    <Hangup/>
</Response>
